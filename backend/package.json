{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend repository for crunch it application.",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && pnpm check-types && tsup src/server.ts",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.mjs",
    "check-types": "tsc --noEmit"
  },
  "type": "module",
  "keywords": [
    "compress",
    "compression",
    "convert",
    "jpeg to png",
    "png to jpeg",
    "svg to png"
  ],
  "author": "karannakra1@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/env": "^5.0.3",
    "@fastify/multipart": "^9.2.1",
    "@fastify/mysql": "^5.0.2",
    "@fastify/websocket": "^11.2.0",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/ws": "^8.18.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5"
  }
}
